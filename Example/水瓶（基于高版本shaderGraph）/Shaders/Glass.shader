					#endif
				#endif
				float3 objToView15 = mul( UNITY_MATRIX_MV, float4( IN.ase_texcoord3.xyz, 1 ) ).xyz;
				float3 normalizeResult16 = normalize( objToView15 );
				float3 ase_worldNormal = IN.ase_texcoord4.xyz;
